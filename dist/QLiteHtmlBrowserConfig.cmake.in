@PACKAGE_INIT@

set(QLITEHTMLBROWSER_VERSION PACKAGE_LIBRARY_VERSION)

set_and_check(QLITEHTMLBROWSER_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@")
set_and_check(QLITEHTMLBROWSER_LIBRARY_DIR "@PACKAGE_LIBRARY_INSTALL_DIR@")

check_required_components(QLiteHtmlBrowser)

include(CMakeFindDependencyMacro)

if(NOT TARGET QLiteHtmlBrowser )
    add_library(QLiteHtmlBrowser SHARED IMPORTED)

endif()

set_property(TARGET QLiteHtmlBrowser PROPERTY
  INTERFACE_INCLUDE_DIRECTORIES ${QLITEHTMLBROWSER_INCLUDE_DIR})

set_property(TARGET QLiteHtmlBrowser PROPERTY
  IMPORTED_LOCATION ${QLITEHTMLBROWSER_LIBRARY_DIR}/${CMAKE_SHARED_LIBRARY_PREFIX}QLiteHtmlBrowser${CMAKE_SHARED_LIBRARY_SUFFIX})

find_dependency( Qt5 @QT_VERSION@ COMPONENTS Widgets Gui Core REQUIRED)


